<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.maple.demo.dao.MessageMapper">

	<!-- 开启二级缓存
	<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/> -->

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.maple.demo.bean.Message">
		<id column="id" property="id" />
		<result column="message" property="message" />
		<result column="send_user" property="sendUser" />
		<result column="reciver_user" property="reciverUser" />
		<result column="create_date" property="createDate" />
		<result column="message_type" property="messageType" />
		<result column="type" property="type" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, message, send_user AS sendUser, reciver_user AS reciverUser, create_date AS createDate, message_type AS messageType, type
    </sql>

</mapper>
