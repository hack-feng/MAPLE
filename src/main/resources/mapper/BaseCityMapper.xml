<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.maple.demo.dao.BaseCityMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.maple.demo.bean.BaseCity">
        <id column="id" property="id" />
        <result column="city_name" property="cityName" />
        <result column="city_code" property="cityCode" />
        <result column="parent_id" property="parentId" />
        <result column="city_name_old" property="cityNameOld" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, city_name, city_code, parent_id, city_name_old
    </sql>

</mapper>
